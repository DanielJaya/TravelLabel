<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_mobile"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.travellabel.View.Main.MainActivity"
        android:label="MainActivity"
        tools:layout="@layout/activity_main" />

    <activity
        android:id="@+id/mapsActivity"
        android:name="com.example.travellabel.View.Map.MapsActivity"
        android:label="activity_maps"
        tools:layout="@layout/activity_maps" />

    <activity
        android:id="@+id/forumActivity"
        android:name="com.example.travellabel.View.Forum.ForumActivity"
        android:label="activity_forum"
        tools:layout="@layout/activity_forum" />
    
    <activity
        android:id="@+id/bookmarkActivity"
        android:name="com.example.travellabel.View.Bookmark.BookmarkActivity"
        android:label="activity_bookmark"
        tools:layout="@layout/activity_bookmark" />

    <activity
        android:id="@+id/ProfileActivity"
        android:name="com.example.travellabel.View.Profile.ProfileActivity"
        android:label="activity_profile"
        tools:layout="@layout/activity_profile" />
</navigation>